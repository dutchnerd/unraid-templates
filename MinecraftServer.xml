<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://github.com/dutchnerd/unraid-templates/blob/master/MinecraftServer.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>MediaApp:Video MediaApp:Music MediaApp:Photos MediaServer:Video MediaServer:Music MediaServer:Photos</Category>
  <Name>Minecraft server</Name>
  <Description>
	minecraft server for docker i have only make the template for unraid docker
  </Description>
  <Overview>
    minecraft server for docker i have only make the template for unraid docker  
  </Overview>
  <Registry>https://hub.docker.com/r/itzg/minecraft-server</Registry>
  <GitHub>https://github.com/itzg/dockerfiles/tree/master/minecraft-server</GitHub>
  <Repository>itzg/dockerfiles/minecraft-server</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>TRANS_DIR</Name>
      <Value>/config/transcode</Value>
    </Variable>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>https://github.com/dutchnerd/unraid-templates/blob/master/images/minecraftserver-icon.png</Icon>
   <ExtraParams></ExtraParams>
</Containers>
