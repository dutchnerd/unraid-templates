<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://github.com/dutchnerd/unraid-templates/blob/master/MinecraftServer.xml</TemplateURL>
  <Beta>False</Beta>
  <Category></Category>
  <Name>Minecraft server</Name>
  <Description>
	minecraft server for docker i have only make the template for unraid docker
  </Description>
  <Overview>
    minecraft server for docker i have only make the template for unraid docker  
  </Overview>
  <Registry>https://hub.docker.com/r/itzg/minecraft-server</Registry>
  <GitHub>https://github.com/itzg/dockerfiles/tree/master/minecraft-server</GitHub>
  <Repository>itzg/minecraft-server</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>VERSION</Name>
      <Value>1.12.2</Value>
    </Variable>
	<Variable>
      <Name>TYPE</Name>
      <Value>FORGE</Value>
    </Variable>
	<Variable>
      <Name>FORGEVERSION</Name>
      <Value></Value>
    </Variable>
	<Variable>
      <Name>EULA</Name>
      <Value>TRUE</Value>
    </Variable>
	<Variable>
      <Name>JVM_OPTS</Name>
      <Value>-Xms1024M -Xmx4096M</Value>
    </Variable>	
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>https://github.com/dutchnerd/unraid-templates/blob/master/images/minecraftserver-icon.png</Icon>
   <ExtraParams></ExtraParams>
</Containers>
